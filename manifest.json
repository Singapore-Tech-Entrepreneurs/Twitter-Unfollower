{
	"name": "Twitter Unfollower",
	"description": "Twitter Unfollow People...",
	"manifest_version": 2,
	"version": "0.1.0",

	"background": {
		"scripts": [ "background.js" ]
	},
	"browser_action": {
		"default_popup": "popup/popup.html"
	},
	
	"minimum_chrome_version": "29",
	
	"permissions": [ 
		"storage",
		"tabs",
		"activeTab",
		"identity",
		"webRequest",
		"webNavigation",
		"contextMenus",
		"*://*.twitter.com/*",
		"notifications"
	]

}
